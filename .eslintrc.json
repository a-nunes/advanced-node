{
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "camelcase": "off",
    "@typescript-eslint/no-namespace": "off",
    "import/prefer-default-export": "off",
    "@typescript-eslint/ban-types": "off",
    "@typescript-eslint/no-explicit-any":"off",
    "max-len": ["error", { "code": 120 }]
  }
}
